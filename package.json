{
  "name": "ts-ethereum-node",
  "version": "0.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "tsc -p ./ --target ES5 && node --prof build/src/index.js",
    "testing": "tsc -p ./ --target ES5 && node build/src/testing.js",
    "watch": "forever ./node_modules/.bin/nodemon --exitcrash --exec \"npm run start\" ./src/index.ts",
    "test": "mocha -r ts-node/register src/**/*_test.ts test/**/*.ts",
    "test-rpc": "cd test/rpc/rpc-tests && make test.eth"
  },
  "keywords": [
    "ethereum",
    "evm",
    "typescript"
  ],
  "license": "",
  "dependencies": {
    "ts-ethereum-vm": "file:../ts-ethereum-vm",
    "@types/bignumber.js": "^5.0.0",
    "@types/immutable": "^3.8.7",
    "@types/node": "^9.4.0",
    "bignumber.js": "^6.0.0",
    "immutable": "^3.8.2",
    "keccak": "^1.4.0",
    "rlp": "^2.0.0",
    "secp256k1": "^3.5.0",
    "typescript": "^2.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "ethereumjs-testing": "^1.0.4",
    "forever": "^0.15.3",
    "mocha": "^5.0.0",
    "nodemon": "^1.17.1",
    "tape": "^4.8.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0"
  }
